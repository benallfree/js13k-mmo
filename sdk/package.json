{
  "name": "js13k-mmo-sdk",
  "version": "0.0.1-rc.6",
  "description": "A simple client SDK for the JS13K MMO Challenge Series that abstracts away the WebSocket connection and state management",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "js13k",
    "mmo",
    "multiplayer",
    "websocket",
    "partysocket",
    "game",
    "sdk"
  ],
  "author": "benallfree",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/benallfree/js13k-mmo-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/benallfree/js13k-mmo-sdk/issues"
  },
  "homepage": "https://github.com/benallfree/js13k-mmo-sdk#readme",
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch"
  },
  "devDependencies": {
    "@types/node": "^24.2.1",
    "partysocket": "^1.1.5",
    "tsdown": "^0.14.1",
    "typescript": "^5.0.0"
  }
}
