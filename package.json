{
  "name": "my-first-worker",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "npm run build && vite preview",
    "deploy": "npm run build && wrangler deploy",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write ."
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.11.3",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "vite": "^7.1.2",
    "wrangler": "^4.29.0"
  },
  "dependencies": {
    "partyserver": "^0.0.72"
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-organize-imports"
    ],
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 120
  }
}
